-- Simple test transformation
-- $Id$
module Test;

metamodel EMFTVM;
transform M : EMFTVM;

rule Test {
	from s : EMFTVM!CodeBlock,
		n : EMFTVM!CodeBlock (
			nestedFor =~ s)
	not nn : EMFTVM!CodeBlock (
			nestedFor =~ n),
		n : EMFTVM!CodeBlock
	to s : EMFTVM!CodeBlock (
		code =~ s.code,
		nested =~ s.nested)
}