<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="MutexB"
    nsURI="http://soft.vub.ac.be/simplegt/2011/MutexB" nsPrefix="MutexB">
  <eClassifiers xsi:type="ecore:EClass" name="Process">
    <eStructuralFeatures xsi:type="ecore:EReference" name="next" eType="#//Process"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="request" upperBound="-1"
        eType="#//Resource"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="count" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"
        changeable="false" volatile="true" transient="true" derived="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Resource">
    <eStructuralFeatures xsi:type="ecore:EReference" name="blocked" upperBound="-1"
        eType="#//Process"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="held_by" eType="#//Process"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="token" eType="#//Process"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="release" eType="#//Process"/>
  </eClassifiers>
</ecore:EPackage>
